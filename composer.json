{
  "name": "convenia/dominio-import",
  "description": "Format employees data to import to Dominio Software",
  "type": "library",
  "license": "MIT",
  "authors": [
      {
          "name": "Eduardo Bizarro",
          "email": "eduardo.bizarro@convenia.com.br"
      }
  ],
  "require": {
    "php": ">=5.4"
  },
  "require-dev": {
      "phpunit/phpunit": "^4.0|^5.0",
      "fabpot/php-cs-fixer": "^1.11",
      "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "Convenia\\DominioImport\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Convenia\\DominioImport\\Tests\\": "tests/"
    }
  },
  "config":{
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
