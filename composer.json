{
  "name": "convenia/dominio-employee-export",
  "description": "Format employees data to import to Dominio Software",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "dominio",
    "folha de pagamento"
  ],
  "authors": [
      {
          "name": "Eduardo Bizarro",
          "email": "edbizarro@gmail.com"
      },
      {
        "name": "Convenia",
        "email": "contato@convenia.com.br"
      }
  ],
  "require": {
    "php": ">=5.6",
    "danielstjules/stringy": "^2.3"
  },
  "require-dev": {
      "phpunit/phpunit": "^4.0|^5.0",
      "friendsofphp/php-cs-fixer": "^2.11",
      "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "Convenia\\Dominio\\EmployeeExport\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Convenia\\Dominio\\EmployeeExport\\Tests\\": "tests/EmployeeExport/"
    }
  },
  "config":{
    "preferred-install": "dist"
  },
  "scripts": {
    "cs-fix": [
      "./vendor/bin/php-cs-fixer fix --verbose --config=./.php_cs ./src ./tests"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
